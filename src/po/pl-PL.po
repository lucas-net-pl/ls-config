#: ls-config.c:45
#, c-format
msgid "Obsluga plikuow konfiguracyjnych\n"
msgstr "Obsługa plików konfiguracyjnych\n"

#: ls-config.c:47
#, c-format
msgid "Skladnia: ls-config [OPCJA]\n"
msgstr "Składnia: ls-config [OPCJA]\n"

#: ls-config.c:48
#, c-format
msgid "Odczytuje i zapisuje dane z plikow konfiguracyjnych\n"
msgstr "Odczytuje i zapisuje dane z plików konfiguracyjnych\n"

#: ls-config.c:49
#, c-format
msgid "w formacie libconfig9\n"
msgstr "w formacie libconfig9\n"

#: ls-config.c:51
#, c-format
msgid "UWAGA: niezbedne jest podanie pliku konfiguracyjnego!\n"
msgstr "UWAGA: niezbędne jest podanie pliku konfiguracyjnego!\n"

#: ls-config.c:53
#, c-format
msgid "Dostepne opcje:\n"
msgstr "Dostępne opcje:\n"

#: ls-config.c:54
#, c-format
msgid "   -f, --file=PLIK       Plik konfiguracyjny do obsluzenia\n"
msgstr "   -f, --file=PLIK       Plik konfiguracyjny do obsłużenia\n"

#: ls-config.c:56
#, c-format
msgid ""
"   -s, --set=SCIEZKA     Ustawienie wartosci konfiguracyjnej o podanej "
"sciezce\n"
msgstr ""
"   -s, --set=SCIEZKA     Ustawienie wartości konfiguracyjnej o podanej "
"ścieżce\n"

#: ls-config.c:57
#, c-format
msgid "   -d, --data=DANE       Wartosc do ustawienia (tylko z -s)\n"
msgstr "   -d, --data=DANE       Wartość do ustawienia (tylko z -s)\n"

#: ls-config.c:58
#, c-format
msgid "   -p, --type=TYP        Typ wartosci do ustawienia\n"
msgstr "   -p, --type=TYP        Typ wartości do ustawienia\n"

#: ls-config.c:60
#, c-format
msgid ""
"   -g, --get=SCIEZKA     Pobranie danych konfiguracyjnych o podanej sciezce\n"
msgstr ""
"   -g, --get=SCIEZKA     Pobranie danych konfiguracyjnych o podanej ścieżce\n"

#: ls-config.c:61
#, c-format
msgid "   -n, --names           Wypisz nazwy zmiennych\n"
msgstr "   -n, --names           Wypisz nazwy zmiennych\n"

#: ls-config.c:62
#, c-format
msgid "   -t, --types           Wypisz typy zmiennych\n"
msgstr "   -t, --types           Wypisz typy zmiennych\n"

#: ls-config.c:63
#, c-format
msgid "   -v, --values          Wypisz wartosci zmiennych\n"
msgstr "   -v, --values          Wypisz wartości zmiennych\n"

#: ls-config.c:64
#, c-format
msgid "   -i, --indexes         Wypisz indeksy zmiennych\n"
msgstr "   -i, --indexex           Wypisz indeksy zmiennych\n"

#: ls-config.c:65
#, c-format
msgid ""
"   -c, --count           Wypisz ilosc elementow (tylko: array, list, group)\n"
msgstr ""
"   -c, --count           Wypisz ilość elementow (tylko: array, list, group)\n"

#: ls-config.c:66
#, c-format
msgid "   -b, --bool-string     Wypisz wartosci logiczne tekstowo\n"
msgstr "   -b, --bool-string     Wypisz wartości logiczne tekstowo\n"

#: ls-config.c:68
#, c-format
msgid "   -q, --quiet           Ciche wyjscie do uzycia w skryptach\n"
msgstr "   -q, --quiet           Ciche wyjście do użycia w skryptach\n"

#: ls-config.c:69
#, c-format
msgid "   -h, --help            Wyswietla niniejszy opis\n"
msgstr "   -h, --help            Wyswietla niniejszy opis\n"

#: ls-config.c:71
#, c-format
msgid "TYP:     typ zmiennej:\n"
msgstr "TYP:     typ zmiennej:\n"

#: ls-config.c:72
#, c-format
msgid "         group  - grupa zmiennych\n"
msgstr "         group  - grupa zmiennych\n"

#: ls-config.c:73
#, c-format
msgid "         array  - tablica zmiennych\n"
msgstr "         array  - tablica zmiennych\n"

#: ls-config.c:74
#, c-format
msgid "         list   - lista zmiennych\n"
msgstr "         list   - lista zmiennych\n"

#: ls-config.c:75
#, c-format
msgid "         int    - liczba calkowita\n"
msgstr "         int    - liczba całkowita\n"

#: ls-config.c:76
#, c-format
msgid "         int64  - liczba calkowita 64 bitowa \n"
msgstr "         int64  - liczba całkowita 64 bitowa \n"

#: ls-config.c:77
#, c-format
msgid "         float  - liczba zmiennoprzecinkowa \n"
msgstr "         float  - liczba zmiennoprzecinkowa \n"

#: ls-config.c:78
#, c-format
msgid "         bool   - wartosc logiczna \n"
msgstr "         bool   - wartość logiczna \n"

#: ls-config.c:79
#, c-format
msgid "         string - lancuch znakowy \n"
msgstr "         string - łąńcuch znakowy \n"

